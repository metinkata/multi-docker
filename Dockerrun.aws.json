{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"273838393/multi-client",
            "hostname":"client",
            "essential":false
        },
        {
            "name":"server",
            "image":"273838393/multi-server",
            "hostname":"api",
            "essential":false
        },
        {
            "name":"worker",
            "image":"273838393/multi-worker",
            "hotname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"273838393/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings":[
                {
                "hostPort":80,
                "containerPort":80
                }
            ],
            "links":["client", "server"]
        }

    ]
}